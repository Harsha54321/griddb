// Online C compiler to run C program online
#include <stdio.h>

int main() {
    // Write C code here
    int a, b, c;
    printf("\n Enter three different numbers");
    scanf("%d%d%d",&a,&b,&c);
    printf("\n a=%d, b=%d, c=%d",a,b,c);
    if(a>b){
        if(a>c){
            printf("\n %d is biggest number",a);
        }
    }
    else
    if(b>c){
        printf("\n %d is biggest number",b);
    }
    else {
        printf ("\n %d is biggest number",c);
    }
    
    return 0;
}
